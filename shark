local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/naypramx/Ui__Project/Script/XeNonUi", true))()
library:CreateWatermark("Arjung | Premium Script")
local CenterHubNo1 = library:CreateWindow("Shark Bite | Premium Script üí≥",Enum.KeyCode.RightControl)
local Tab = CenterHubNo1:CreateTab("GeneralÔ∏è")
local Sector1 = Tab:CreateSector("Farm ‚è©","left")
-------------- funtion



function tp(locat)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = locat
end

local function getEnemy()
    local dist, thing = math.huge
    for i,v in pairs(workspace:GetChildren()) do
        if v:IsA"Model" and v:FindFirstChild("Bite") then
            local mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Hitbox.Position).Magnitude
            if mag < dist then 
                dist = mag 
                thing = v 
            end
        end
    end
    return thing
end

local function getTool()
    for i,v in next, game.Players.LocalPlayer.Character:GetChildren() do
        if v:IsA"Tool" then
            return v
        end
    end
end




