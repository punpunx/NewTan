local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("UZAMA กังกัง HUB", "DarkTheme")
local Tab = Window:NewTab("Farm")
local Section = Tab:NewSection("Farm Select")
Section:NewToggle("Sand", "ToggleInfo", function(state)
	if state then
		_G.pun = true
		while _G.pun do wait(0.1)
			pcall(function()
				local char = game.Players.LocalPlayer.Character.HumanoidRootPart
				for i,v in pairs(game:GetService("Workspace").Enemy.Sahara:GetDescendants()) do 
					if v.Name == 'HumanoidRootPart' and v.Parent.Humanoid.Health > 0 then
						char.CFrame = v.CFrame * CFrame.new(0,0,3)
						game:GetService'VirtualUser':CaptureController()
						game:GetService'VirtualUser':Button1Down(Vector2.new(686, 352))
						game:GetService("Players").LocalPlayer.Backpack.Haki.Haki:FireServer("Haki")
						game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack.weap)

					end
				end
			end)
		end

	else
		_G.pun = false
		while _G.pun do wait(0.1)
			pcall(function()
				local char = game.Players.LocalPlayer.Character.HumanoidRootPart
				for i,v in pairs(game:GetService("Workspace").Enemy.Sahara:GetDescendants()) do 
					if v.Name == 'HumanoidRootPart' and v.Parent.Humanoid.Health > 0 then
						char.CFrame = v.CFrame * CFrame.new(0,0,3)
						game:GetService'VirtualUser':CaptureController()
						game:GetService'VirtualUser':Button1Down(Vector2.new(686, 352))
						game:GetService("Players").LocalPlayer.Backpack.Haki.Haki:FireServer("Haki")
						game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack.weap)

					end
				end
			end)
		end
	end
end)
