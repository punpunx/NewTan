local Tab = Window:NewTab("Weapon")
local Section = Tab:NewSection("Weapon Select")
Section:NewToggle("Katana", "ToggleInfo", function(state)
    if state then
        _G.wep = true
while _G.wep do wait(0)
    pcall(function()
    game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack.Katana)
    end)
    end
    
    else
        _G.wep = false
while _G.wep do wait(0)
    pcall(function()
    game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack.Katana)
    end)
    end
    end
end)
